+        format.json { render :show, status: :ok, location: @tag }
+      else
+        format.html { render :edit }
+        format.json { render json: @tag.errors, status: :unprocessable_entity }
+      end
+    end
+  end
+
+  # DELETE /tags/1
+  # DELETE /tags/1.json
